<%- | String  $server_ip,
      String  $path_to_log_file,
      Hash    $userparameter = {},
| -%>
PidFile=/var/run/zabbix/zabbix_agentd.pid
LogFile=<%= $path_to_log_file %>
LogFileSize=0

Server=<%= $server_ip %>
ServerActive=<%= $server_ip %>

HostnameItem=system.hostname
<% $options.each |$key, $value| { -%>
UserParameter=<%= $key %>, <%= $value %>
<% } -%>
